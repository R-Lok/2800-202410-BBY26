<%- include("template/header.ejs") %>
<%- include("template/headerBar.ejs") %>
<div class="mt-3 ms-4">
    <form action="/login" method="post">
        <div class="form-group m-2">
            <label for="text">Login ID</label>
            <input type="text" class="form-control" id="text" name="loginId" aria-describedby="emailHelp"
                placeholder="Enter Account ID">
            <small id="loginIdHelp" class="form-text text-muted">example: name</small>
        </div>
        <div class="form-group m-2">
            <label for="password">Password</label>
            <input type="password" class="form-control" id="password" name="password" placeholder="Enter password">
            <small id="passworldHelp" class="form-text text-muted">Alphabet or Number 3 to 20 characters</small>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
<%- include("template/footerBar.ejs") %>
<%- include("template/footer.ejs") %>
